    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.6.5",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.2",
    "prettier": "3.1.1"
